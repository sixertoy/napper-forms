{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier",
    "simple-import-sort",
    "import",
    "sort-destructure-keys",
    "sort-keys-fix"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "simple-import-sort/sort": 2,
    "sort-destructure-keys/sort-destructure-keys": 2,
    "sort-imports": "off",
    "sort-keys": ["error", "asc"],
    "simple-import-sort/sort": 2,
    "sort-imports": "off",
    "sort-keys-fix/sort-keys-fix": ["error", "asc"],
    "react/jsx-props-no-spreading": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": false,
        "shorthandFirst": true,
        "shorthandLast": false,
        "ignoreCase": true,
        "noSortAlphabetically": false,
        "reservedFirst": false
      }
    ]
  }
}
